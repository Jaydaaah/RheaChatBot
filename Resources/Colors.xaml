<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="AccentColor" Color="MediumPurple"/>
    <SolidColorBrush x:Key="SubAccentColor" Color="Lavender"/>
    <SolidColorBrush x:Key="PrimaryForeground" Color="Black"/>
    <SolidColorBrush x:Key="SecondaryForeground" Color="White"/>
    <SolidColorBrush x:Key="Background" Color="White"/>
</ResourceDictionary>